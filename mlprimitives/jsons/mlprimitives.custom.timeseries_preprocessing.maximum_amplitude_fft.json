{
    "name": "mlprimitives.custom.timeseries_preprocessing.maximum_amplitude_fft",
    "contributors": [
        "Alexander Geiger <geigera@mit.edu>"
    ],
    "description": "Compute max amplitude of FFT of each interval and compare smoothened amplitude of last window_size intervals with following interval",
    "classifiers": {
        "type": "estimator",
        "subtype": "regressor"
    },
    "modalities": [
        "timeseries"
    ],
    "primitive": "mlprimitives.custom.timeseries_preprocessing.maximum_amplitude_fft",
    "produce": {
        "args": [
            {
                "name": "X",
                "type": "ndarray"
            },
            {
                "name": "index",
                "type": "ndarray"
            }
        ],
        "output": [
            {
                "name": "y_hat",
                "type": "ndarray"
            },
            {
                "name": "y",
                "type": "ndarray"
            },
            {
                "name": "index",
                "type": "ndarray"
            },
            {
                "name": "target_index",
                "type": "ndarray"
            }
        ]
    },
    "hyperparameters": {
        "fixed": {
            "interval": {
                "type": "int",
                "default": 7
            },
            "window_size": {
                "type": "int",
                "default": 3
            }
        }
    }
}
