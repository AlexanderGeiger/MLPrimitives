{
    "name": "mlprimitives.custom.timeseries_preprocessing.butterworth_filter",
    "contributors": [
        "Alexander Geiger <geigera@mit.edu>"
    ],
    "description": "Apply Butterworth filter to signal",
    "classifiers": {
        "type": "preprocessor",
        "subtype": "feature_extractor"
    },
    "modalities": [
        "timeseries"
    ],
    "primitive": "mlprimitives.custom.timeseries_preprocessing.butterworth_filter",
    "produce": {
        "args": [
            {
                "name": "X",
                "type": "ndarray"
            }
        ],
        "output": [
            {
                "name": "X",
                "type": "ndarray"
            },
            {
                "name": "index",
                "type": "ndarray"
            }
        ]
    },
    "hyperparameters": {
        "fixed": {
            "time_column": {
                "type": "str or int",
                "default": 0
            },
            "N": {
                "type": "int",
                "default": 5
            },
            "Wn": {
                "type": "float",
                "default": 0.05
            }
        }
    }
}
